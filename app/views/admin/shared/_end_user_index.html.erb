<table class='table'>
  <thead>
    <tr>
      <th class="col-1 text-center">ID</th>
      <th class="col-3 text-center">プロフィール画像</th>
      <th>ユーザー名</th>
      <th>アドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% end_users.each do |end_user| %>
    <tr>
      <td class="text-center">
        <%= end_user.id %><br/>
        (<%= end_user.created_at.strftime('%Y/%m/%d') %>)
      </td>
      <td class="text-center"><%= image_tag end_user.get_profile_image, size: '50x50' %></td>
      <td><%= link_to admin_end_user_path(end_user.id) do %><%= end_user.name %><% end %></td>
      <td><%= end_user.email %></td>
      <td class= "<%= end_user.is_deleted ? 'text-secondary' : 'text-success' %>"><%= end_user.is_deleted ? '退会済み' : '有効' %></td>
    </tr>
    <% end %>
  </tbody>
</table>